{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [
    "prettier",
    "vue"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-empty": "off"
  },
  "overrides": [
    {
      "files": [
        "./scripts/*.js"
      ],
      "env": {
        "node": true
      }
    }
  ]
}
